
public class Student {
	
	private String name;
	private int[] marks;


	public Student(String name, int[] marks) {
		this.name = name;
		this.marks = marks;
		
	}


	public int getNumberOfMarks() {
		return marks.length;
	}


	public int getTotalSumOfMarks() {
		int sum = 0;
		for(int i=0; i<marks.length;i++) {
			sum += marks[i];
		}
		return sum;
	}
}	
